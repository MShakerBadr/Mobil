<configuration>
  <system.web>
    <compilation targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <customErrors mode="Off"/>
  </system.web>
  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
    <add key="MobilAPIURL" value="http://localhost:1466" />
  </appSettings>
  <system.webServer>
    <defaultDocument>
      <files>
        <add value="ResetPassword.aspx" />
      </files>
    </defaultDocument>
  </system.webServer>
</configuration>

