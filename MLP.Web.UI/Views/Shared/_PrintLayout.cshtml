<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <style>
        .displaybox {
            margin: 5px;
            border: 1px solid #eeeeee;
        }

        body {
            /*position: fixed;*/
            top: 10px;
            bottom: 30px;
            left: 0px;
            right: 0;
            padding: 20px;
            padding-top: 20px;
            padding-top: 20px;
            padding-top: 0px;
            overflow-y: auto;
            overflow-x: hidden;
            background-color: white;
            
        }
        table{
                display: table;
               border-collapse: separate;
               border-spacing: 0px;
               border-color: grey;
        }
        img {
            vertical-align: unset;
        }
       .watermark {
                color: #d0d0d0;
                font-size: 150pt;
                -webkit-transform: rotate(-45deg);
                -moz-transform: rotate(-45deg);
                position: fixed;
                width: 100%;
                /*height: 100%;*/
                margin: 0;
                opacity:0.5;
                z-index: 2;
                left:15%;
                top:0%;
            }
       .page-footer{
           position:absolute;
       }
    </style>
    <title>Print</title>
 
</head>
<body>
    <div>
        
            <div class="row" style="background-color: grey;margin: 10px;">
                <div class="col-md-12">
                    <img  width="100%" height="50" src="~/Images/unnamed.png" />
                </div>
            </div>
            <div style="padding-left:42%;">
                <div class="Dtd Dnormal">
                    <label> </label>
                </div>
            </div>
            @RenderBody()
    </div>
    <div class="page-footer">
        <div class="page-footer-inner">
            <img width="100%" height="100" src="~/Images/Footer.png" />
        </div>
    </div>
    
    <!-- jQuery -->
    <script  type="text/javascript">
       
        
            window.print();
    </script>
  
</body>

</html>
